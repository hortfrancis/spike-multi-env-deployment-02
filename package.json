{
  "name": "spike-multi-env-deployment-02",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "uat": "vite --mode uat",
    "build:dev": "CLOUDFLARE_ENV=dev vite build --mode dev",
    "build:uat": "CLOUDFLARE_ENV=uat vite build --mode uat",
    "build:pp": "CLOUDFLARE_ENV=pp vite build --mode pp",
    "build:prod": "CLOUDFLARE_ENV=prod vite build --mode prod",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.23.0",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.18"
  }
}
